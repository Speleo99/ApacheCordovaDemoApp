<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tedra.toolbox" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="Orientation" value="portrait" />
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="Battery">
        <param name="android-package" value="org.apache.cordova.batterystatus.BatteryListener" />
    </feature>
    <feature name="Vibration">
        <param name="android-package" value="org.apache.cordova.vibration.Vibration" />
    </feature>
    <feature name="Accelerometer">
        <param name="android-package" value="org.apache.cordova.devicemotion.AccelListener" />
    </feature>
    <feature name="Compass">
        <param name="android-package" value="org.apache.cordova.deviceorientation.CompassListener" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="BatteryPlugin">
        <param name="android-package" value="com.blueshift.cordova.battery.BatteryPlugin" />
    </feature>
    <name>toolbox</name>
    <description>
        Une application Cordova pour tester les plugins et la communication avec les fonctionnalités natives du téléphone. 
    </description>
    <author email="ted.ramoni@gmail.com" href="">
        Tedra Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="http://*.google.com" />
    <access origin="https://*.google.com" />
    <access origin="geo:*" launch-external="yes" />
    <access origin="*.google.com" />
    <access origin="*.googleapis.com" />
    <access origin="*.gstatic.com" />
    <access origin="*.googleusercontent.com" />
    <access origin="google.com" subdomains="true"/>
    <access origin="googleapis.com" subdomains="true"/>
    <access origin="gstatic.com" subdomains="true"/>
    <access origin="googleusercontent.com" subdomains="true"/>
    <access origin="*.pushwoosh.com" />
</widget>
